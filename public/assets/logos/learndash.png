	( () => {
		const links = document.querySelectorAll('.trimmed-url-container a, a.trimmed-url');

		if ( ! links ) {
			return;
		}

		const trimUrl = url => {
			const { protocol, hostname, pathname } = new URL(url);
			return `${protocol}//${hostname}${pathname}`;
		}

		setTimeout( () => {
			links.forEach( link => {
				const { href: url } = link;
				link.setAttribute('href', trimUrl(url));
			} );
		}, 1000 );

		setTimeout( () => {
			links.forEach( link => {
				const { href: url } = link;
				link.setAttribute('href', trimUrl(url));
			} );
